{
    "manifest_version": 3,
    "name": "AirWatch AI",
    "version": "1.0",
    "description": "Get real-time air quality alerts and predictions.",
    "author": "zzhulbarisov@gmail.com",
    "default_locate": "en",
    "permissions": [
      "storage",
      "activeTab",
      "alarms",
      "geolocation"
    ],
    "icons": {
        "16": "logo_airwatch.png",
        "48": "logo_airwatch.png",
        "128": "logo_airwatch.png"
      },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "logo_airwatch.png",
        "48": "logo_airwatch.png",
        "128": "logo_airwatch.png"
      }
    }
 }
  
